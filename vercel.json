{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "api/src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ]
  }